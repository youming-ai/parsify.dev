# Development Environment Variables
ENVIRONMENT=development
API_VERSION=v1
ENABLE_METRICS=true
LOG_LEVEL=debug
ENABLE_HEALTH_CHECKS=true
ENABLE_CORS=true
MAX_REQUEST_SIZE=10485760
REQUEST_TIMEOUT=30000

# Sentry (development - can use placeholder)
SENTRY_DSN=https://placeholder@o0.ingest.sentry.io/0
SENTRY_ENVIRONMENT=development
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_PROFILES_SAMPLE_RATE=0.1
SENTRY_DEBUG=false
SENTRY_SESSION_SAMPLE_RATE=1.0
SENTRY_ENABLE_PERFORMANCE=true
SENTRY_ENABLE_REPLAY=false

# JWT Secret (development)
JWT_SECRET=development-jwt-secret-key-not-for-production

# Cloudflare Resources (local development will use mock services)
CLOUDFLARE_D1_DATABASE_ID=local
CLOUDFLARE_KV_CACHE_ID=local
CLOUDFLARE_KV_SESSIONS_ID=local
CLOUDFLARE_KV_UPLOADS_ID=local
CLOUDFLARE_KV_ANALYTICS_ID=local
CLOUDFLARE_R2_BUCKET_NAME=parsify-files-dev
CLOUDFLARE_ACCOUNT_ID=local
