    "sentry:all": "node ../../scripts/sentry-release.js all"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@sentry/cloudflare": "^8.0.0",
    "hono": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  }
}
