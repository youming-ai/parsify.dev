{
  "name": "parsify-dev",
  "version": "1.0.0",
  "private": true,
  "description": "Essential Developer Tools - Privacy-first utilities running entirely in your browser.",
  "homepage": "https://parsify.dev",
  "repository": "https://github.com/youming-ai/parsify.dev",
  "author": "Youming <ikashue@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vinext dev",
    "build": "vinext build",
    "start": "vinext start",
    "preview": "bunx wrangler dev",
    "deploy": "vinext deploy",
    "deploy:cf": "vinext deploy",
    "lint": "bunx biome check ./src",
    "lint:fix": "bunx biome check --fix ./src",
    "format": "bunx biome format --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:check": "vitest --coverage",
    "prepare": "husky",
    "clean": "rm -rf node_modules dist .wrangler"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": ["biome check --fix --no-errors-on-unmatched"],
    "*.{ts,tsx}": ["vitest related --run --passWithNoTests"]
  },
  "dependencies": {
    "@ai-sdk/groq": "^3.0.24",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@tanstack/react-virtual": "^3.13.19",
    "ai": "^6.0.99",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "dompurify": "^3.3.1",
    "jsonpath-plus": "^10.4.0",
    "next": "^15.5.12",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "smol-toml": "^1.6.0",
    "sonner": "^2.0.7",
    "spark-md5": "^3.0.2",
    "tailwind-merge": "^2.6.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/spark-md5": "^3.0.5",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitejs/plugin-rsc": "^0.5.20",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "happy-dom": "^20.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vinext": "^0.0.5",
    "vite": "^7.3.1",
    "vitest": "4.0.18",
    "wrangler": "^4.68.1"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.3.5",
  "type": "module"
}
